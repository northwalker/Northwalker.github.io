{
  "name": "northwalker-github-io",
  "version": "0.2.0",
  "description": "Github website for Northwalker",
  "main": "index.js",
  "scripts": {
    "start": "npm run dev",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --inline --open --hot --config webpack.config.js",
    "watch": "npm run dev",
    "prebuild": "rm -rf docs",
    "build": "cross-env NODE_ENV=production webpack --progress --hide-modules --config webpack.config.js",
    "postbuild": "npm run copyIndex",
    "lint": "eslint ./src/*.js",
    "copyIndex": "mv ./dist/index.html ./"
  },
  "author": "Northwalker",
  "homepage": "https://github.com/northwalker/Northwalker.github.io#readme",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/northwalker/Northwalker.github.io/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/northwalker/Northwalker.github.io.git"
  },
  "dependencies": {
    "normalize.css": "^7.0.0"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.3",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.7",
    "eslint": "^4.6.1",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.9.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "html-webpack-inline-source-plugin": "^0.0.9",
    "html-webpack-plugin": "^2.30.1",
    "node-sass": "^4.5.3",
    "path": "^0.12.7",
    "postcss-cssnext": "^3.0.2",
    "postcss-import": "^10.0.0",
    "postcss-loader": "^2.0.6",
    "sass-loader": "^6.0.6",
    "script-ext-html-webpack-plugin": "^1.8.5",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.9",
    "webpack": "^3.5.5",
    "webpack-dev-server": "^2.7.1"
  }
}
